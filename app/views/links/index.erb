<h1>Links</h1>
<form action="/tags/:name">
  <label for="name">
  <input type="text" name="name"/>
  </label>
  <input type="submit" value="Search By Tag"/>
</form>
<ul id="links">
  <%@links.each do |link| %>
    <li> Title: <%= link.title %></li>
    <li> URL: <%= link.url %></li>
  <% end %>
</ul>
